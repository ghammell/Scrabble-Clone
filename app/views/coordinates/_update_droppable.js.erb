var dropResetter = {
  coords: JSON.parse("<%= escape_javascript (@droppable.to_json.html_safe) %>"),
  reset: function() {
    $('.coordinate').droppable('disable')
    for (i=0; i < this.coords.length; i++) {
      coordinate_div = '#coordinate_' + this.coords[i].id
      $(coordinate_div).droppable('enable')
    }
  }
}